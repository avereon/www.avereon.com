<jnlp spec="1.0+" codebase="http://www.parallelsymmetry.com/webstart/clearcadd/" href="http://www.parallelsymmetry.com/webstart/clearcadd/clearcadd.jnlp">
  <information>
    <title>ClearCadd</title>
    <vendor>Parallel Symmetry</vendor>
    <homepage href="http://www.parallelsymmetry.com/projects"/>
    <description>Computer-aided modeling application.</description>
    <description kind="short">Computer-aided modeling.</description>
    <icon href="http://www.parallelsymmetry.com/webstart/clearcadd/splash.png" kind="splash"/>
    <icon href="http://www.parallelsymmetry.com/webstart/clearcadd/clearcadd.64.png" height="64" width="64" kind="default"/>
    <icon href="http://www.parallelsymmetry.com/webstart/clearcadd/clearcadd.32.png" height="32" width="32" kind="default"/>
    <offline-allowed/>
  </information>
  <security>
    <all-permissions/>
  </security>
  <update check="timeout" policy="always"/>
  <resources>
    <j2se href="http://java.sun.com/products/autodl/j2se" version="1.6+"/>
    <jar href="http://mvn.parallelsymmetry.com/release/com/parallelsymmetry/clearcadd/1.0/clearcadd-1.0.jar" download="eager" main="false"/>
    <jar href="http://mvn.parallelsymmetry.com/release/com/parallelsymmetry/clearview/1.0/clearview-1.0.jar" download="eager" main="false"/>
    <jar href="http://mvn.parallelsymmetry.com/release/com/parallelsymmetry/service/1.0/service-1.0.jar" download="eager" main="false"/>
    <jar href="http://mvn.parallelsymmetry.com/release/com/parallelsymmetry/utility/1.0/utility-1.0.jar" download="eager" main="false"/>
    <extension name="jogl" href="http://download.java.net/media/jogl/builds/archive/jsr-231-webstart-current/jogl.jnlp" />
    <property name="apple.awt.graphics.UseOpenGL" value="true"/>
    <!-- property name="apple.awt.graphics.UseQuartz" value="true"/ -->
  </resources>
  <application-desc main-class="com.parallelsymmetry.clearcadd.Main"/>
</jnlp>
